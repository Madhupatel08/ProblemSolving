public int exactly3Divisors(int N)
    {
    int ans = 0;
    boolean prime = true;
    for(int i=2; i*i<=N; i++)
    {
        int j=2;
        while(j*j<=i)
        {
            if(i % j == 0)
            {
                prime = false;
                break;
            }
            j++;
        }
        if(prime==true)
        {
            ans++;
        }
        prime = true;
    }
    return ans;  
    }
